

{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "./src/*"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "store/*": [
        "./src/store/*"
      ],
      "svg/*": [
        "./src/assets/svg/*"
      ],
    },
    "target": "es2016", 
    "jsx": "react-jsx",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
}